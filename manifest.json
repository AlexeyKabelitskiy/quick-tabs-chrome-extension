{
    "name": "Quick Tabs",
    "version": "0.1.0",

    "description": "A quick tab list and switch plugin inspired by the intellij IDEA java IDE",

    "background_page": "background.html",

    "browser_action": {
        "default_icon": "icon.gif",
        "default_title": "Quick tabs",      // optional; shown in tooltip
        "popup": "popup.html",
        "keyboard": "Ctrl + e"
    },

    "permissions": ["tabs"],


    "icons": {
		"32": "icon.gif"
    }
}